<xsl:stylesheet version='1.1'	xmlns:xsl='http://www.w3.org/1999/XSL/Transform'	xmlns:build='http://xsltsl.org/xsl/build/1.0'>  <xsl:import href='xsl2jref.xsl'/>  <xsl:param name='includestylesheets' select='0'/>  <xsl:template match='/'>    <xsl:apply-templates/>  </xsl:template>  <xsl:template match='build:sources'>    <xsl:for-each select='build:stylesheet'>      <xsl:message>Extracting <xsl:value-of select='.'/> to <xsl:value-of select='concat(substring-before(substring(., 4), ".xsl"), ".xml")'/>&#10;</xsl:message>      <xsl:document href='{substring-before(substring(., 4), ".xsl")}.xml'>	<xsl:apply-templates select='document(.)'/>      </xsl:document>    </xsl:for-each>  </xsl:template></xsl:stylesheet>